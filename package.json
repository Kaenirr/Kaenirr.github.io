		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"gh-pages": "^6.1.1",
		"sass": "^1.71.1",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"unocss": "^0.58.5",
		"vite": "^5.0.3"
	},
	"type": "module"
}
